{
  "spaces": 2,
  "generator-cli": {
    "version": "5.3.0",
    "generators": { 
      "angular": {
        "generatorName": "typescript-angular",
        "output": "#{cwd}/generator/generated",
        "glob": "generator/api-schema.json",
        "additionalProperties": {
          "npmName": "client",
          "npmVersion": "1.0.0",
          "ngVersion": "15.1.3",
          "supportsES6": true,
          "fileNaming": "kebab-case",
          "modelFileSuffix": ".model",
          "withoutPrefixEnums": true,
          "withSeparateModelsAndApi": false
        }
      }
    }
  }
}
